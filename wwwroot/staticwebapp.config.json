{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/css/*", "/_framework/*", "/*.js", "/*.json"]
  },
  "globalHeaders": {
    "Cache-Control": "public, max-age=3600"
  },
  "mimeTypes": {
    ".dll": "application/octet-stream",
    ".wasm": "application/wasm",
    ".json": "application/json"
  }
}
